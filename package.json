{
  "name": "generic-filehandle",
  "description": "uniform interface for accessing binary data from local files, remote HTTP resources, and browser Blob data",
  "version": "2.1.0",
  "main": "dist/generic-filehandle.cjs.js",
  "module": "dist/generic-filehandle.esm.js",
  "types": "dist/generic-filehandle.cjs.d.ts",
  "browser": {
    "./dist/generic-filehandle.cjs.js": "./browser/dist/generic-filehandle-browser.cjs.js",
    "./dist/generic-filehandle.esm.js": "./browser/dist/generic-filehandle-browser.esm.js",
    "./dist/generic-filehandle.cjs.d.ts": "./browser/dist/generic-filehandle-browser.cjs.d.ts"
  },
  "repository": "GMOD/generic-filehandle",
  "license": "MIT",
  "author": {
    "name": "Colin Diesh",
    "email": "colin.diesh@gmail.com",
    "url": "https://github.com/cmdcolin"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "browser"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "lint": "eslint --ext .ts src test",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "preconstruct build",
    "watch": "preconstruct watch",
    "preversion": "npm run lint && npm test && npm run build",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "postinstall": "patch-package"
  },
  "keywords": [
    "bionode",
    "biojs",
    "ucsc",
    "genomics"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "browser.ts"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "file-uri-to-path": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@preconstruct/cli": "^2.1.0",
    "@types/fetch-mock": "^7.3.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^13.13.5",
    "@types/range-parser": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "cross-fetch": "^3.0.3",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fetch-mock": "^9.7.0",
    "jest": "^26.0.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.0.5",
    "range-parser": "^1.2.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "standard-changelog": "^2.0.24",
    "tenacious-fetch": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
